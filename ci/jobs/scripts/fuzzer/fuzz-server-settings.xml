<clickhouse>
    <!-- Allow merge tree settings to avoid errors while fuzzing definitions of tables -->
    <merge_tree>
        <allow_nullable_key>1</allow_nullable_key>
    </merge_tree>
    <!-- Definitions to be used by BuzzHouse -->
    <allow_experimental_transactions>1</allow_experimental_transactions>
    <keeper_map_path_prefix>/keeper_map_tables</keeper_map_path_prefix>
    <named_collections>
        <local />
    </named_collections>
    <remote_servers>
        <cluster0>
            <shard>
                <replica>
                    <host>localhost</host>
                    <port>9000</port>
                </replica>
            </shard>
        </cluster0>
    </remote_servers>
    <shared_database_catalog />
    <distributed_ddl>
        <path>/clickhouse/task_queue/ddl</path>
        <replicas_path>/clickhouse/task_queue/replicas</replicas_path>
    </distributed_ddl>
    <backups>
        <allowed_disk>default</allowed_disk>
        <allowed_path>/var/lib/clickhouse/</allowed_path>
        <allowed_path>/var/lib/clickhouse/user_files/</allowed_path>
    </backups>
    <tmp_path>/var/lib/clickhouse/tmp/</tmp_path>
</clickhouse>
